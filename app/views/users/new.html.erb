<h1>Ajouter User</h1>
<br>
<br>

<div class="col-lg-1 col-offset-6 centered">
<%= form_for @user do |e| %>
  <div class="form-group" >
    <label>User_id : </label>
    <%= e.text_field :user_id %>
  </div>
  <div class="form-group">
    <label>App : </label>
    <%= e.text_field :app %>
  </div>
  <div class="form-group">
    <button type="submit" class="btn btn-primary">Ajouter</button>
  </div>
<%end %>
  </div>
